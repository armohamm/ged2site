<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
    "http://www.w3.org/TR/html4/loose.dtd">
<html lang="en">
	<head>
		[% IF census %]
			<title>[% census.0.census %] entries in [% SiteTitle.join(' ') %]</title>
		[% ELSE %]
			<title>Censuses for [% SiteTitle.join(' ') %]</title>
		[% END %]
		<meta http-equiv="Content-Language" content="English">
		<!--
		<meta name="msvalidate.01" content="deadbeef" />
		-->
	</head>
	<body>
		<center>
			[% IF census %]
				<h1>[% census.0.census %] entries in [% SiteTitle.join(' ') %]</h1>
			[% ELSE %]
				<h1>Censuses for [% SiteTitle.join(' ') %]</h1>
			[% END %]
			[% IF error %]
				<p><font color="red" size="+2">[% error %]</font><center></p>
			[% END %]
			[ <a href="?page=people&home=1">Home</a> ] | [ <a href="?page=censuses">Census Index</a> ] | [ <a href="?page=surnames">Surname Index</a>
			[% IF censuses %]
				[% FOREACH census IN censuses %]
					<a href="?page=censuses&census=[%- census | uri -%]">[% census %]</a>
				[% END %]
			[% ELSIF people %]
				<ol>
					[% FOREACH person IN people %]
						<li>
							<a href="?page=people&entry=[%- person.entry %]">[% person.title %]</a>
						</li>
					[% END %]
				</ol>
			[% END %]
		</center>
		<p align="right">
			<i>
			Generated by <a href="https://github.com/nigelhorne/gedsite">gedsite</a>.
			Last updated on Jul 22, 2016
			</i>
		</p>
	</body>
</html>
