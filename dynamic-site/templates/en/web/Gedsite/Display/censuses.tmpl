<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
    "http://www.w3.org/TR/html4/loose.dtd">
<html lang="en">
	<head>
		[% IF census %]
			<title>[% census.entry %]</title>
		[% ELSE %]
			<title>Censuses for [% SiteTitle.join(' ') %]</title>
		[% END %]
		<meta http-equiv="Content-Language" content="English">
		<!--
		<meta name="msvalidate.01" content="deadbeef" />
		-->
	</head>
	<body>
		<center>
			[% IF census %]
				<h1>[% census.entry %]</h1>
			[% ELSE %]
				<h1>Censuses for [% SiteTitle.join(' ') %]</h1>
			[% END %]
			[ <a href="?page=people&home=1">Home</a> ] | [ <a href="?page=censuses">Census Index</a> ]
			[% IF censuses %]
				[% FOREACH census IN censuses %]
					<a href="?page=censuses?census=[%- census | uri -%]">[% census %]</a>
				[% END %]
			[% END %]
		</center>
		<p align="right">
			<i>
			Generated by <a href="https://github.com/nigelhorne/gedsite">gedsite</a>.
			Last updated on Jul 22, 2016
			</i>
		</p>
	</body>
</html>
