<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
    "http://www.w3.org/TR/html4/loose.dtd">
<html lang="en">
	<head>
		[% IF surname %]
			<title>Entries for the surname [% surname.0.surname %] in [% SiteTitle.join(' ') %]</title>
		[% ELSE %]
			<title>Surnames for [% SiteTitle.join(' ') %]</title>
		[% END %]
		<meta http-equiv="Content-Language" content="English">
		<!--
		<meta name="msvalidate.01" content="deadbeef" />
		-->
	</head>
	<body>
		<center>
			[% IF surname %]
				<h1>Entries for the surname [% surname.0.surname %] in [% SiteTitle.join(' ') %]</h1>
			[% ELSE %]
				<h1>Surnames for [% SiteTitle.join(' ') %]</h1>
			[% END %]
			[ <a href="?page=people&home=1">Home</a> ] | [ <a href="?page=censuses">Census Index</a> ] | [ <a href="?page=surnames">Surname Index</a>
			[% IF surnames %]
				[% FOREACH surname IN surnames %]
					<a href="?page=surnames&surname=[%- surname | uri -%]">[% surname %]</a>
				[% END %]
			[% ELSIF people %]
				<ol>
					[% FOREACH person IN people %]
						<li>
							<a href="?page=people&entry=[%- person.entry %]">[% person.title %]</a>
						</li>
					[% END %]
				</ol>
			[% END %]
		</center>
		<p align="right">
			<i>
			Generated by <a href="https://github.com/nigelhorne/gedsite">gedsite</a>.
			Last updated on Jul 22, 2016
			</i>
		</p>
	</body>
</html>
